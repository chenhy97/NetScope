        table tb_set_sink {
            key = {
                hdr.ipv4.dstAddr: exact;
            }
            actions = {
                int_set_sink;
                NoAction;
            }
            default_action = NoAction;
        }

        apply{
            tb_set_source.apply();
            tb_set_sink.apply();
        }

    }
