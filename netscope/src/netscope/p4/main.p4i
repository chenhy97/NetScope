}

control detect_int_source_sink (
    inout headers hdr,
    inout metadata meta,
    inout standard_metadata_t standard_metadata) {
        action int_set_source () {
            meta.int_meta.source = true;
        }

        action int_set_sink () {
            meta.int_meta.sink = true;
        }

        table tb_set_source {
            key = {
